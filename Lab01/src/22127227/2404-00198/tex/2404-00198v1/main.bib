  publisher = {{American Chemical Society}},
  issn = {1932-7447},
  doi = {10.1021/acs.jpcc.8b03030},
  urldate = {2021-08-03},
  abstract = {Metallic nanostructures are well known for their potential to enhance resonant energy transfer between chromophores, mediated by their plasmonic field. In most cases, the distances for efficient energy transfer are determined by the dimensions of the structure, making dissipation in the metal dominant when long-range energy transfer is desired. Here, we propose and study a new mechanism for long-distance energy transfer, which can lead to highly efficient energy transfer over distances comparable to the optical wavelength, based on hybrid photonic\textendash plasmonic modes in metal-dielectric structures. We study such structures theoretically and characterize the energy-transfer processes in them, showing that within these structures, energy can be funneled with {$\sim$}25\% efficiency over a range of about 150 nm, with minimal dependence on the location of the acceptor molecules inside the structure. Finally, we demonstrate this new mechanism experimentally, providing a proof-of-concept for long-distance energy transfer in such structures. Our multilayer system is compatible with standard photovoltaic and organic light-emitting diodes and therefore the mechanism presented can be readily employed in such organic optoelectronic devices to improve their performance.},
  file = {C\:\\Users\\s3542681\\Zotero\\storage\\HDY2LULJ\\Akulov et al. - 2018 - Long-Distance Resonant Energy Transfer Mediated by.pdf;C\:\\Users\\s3542681\\Zotero\\storage\\WADUN8BH\\acs.jpcc.html}
}

@article{Georgiou_ACIE2021,
  title = {Ultralong-Range Polariton-Assisted Energy Transfer in Organic Microcavities},
  author = {Georgiou, Kyriacos and Jayaprakash, Rahul and Othonos, Andreas and Lidzey, David G.},
  year = {2021},
  journal = {Angewandte Chemie International Edition},
